html
  head
    link(href="https://fonts.googleapis.com/icon?family=Material+Icons", rel="stylesheet")
    link(href="https://code.getmdl.io/1.1.3/material.indigo-pink.min.css", rel="stylesheet")
    link(href="https://rawgit.com/MEYVN-digital/mdl-selectfield/master/mdl-selectfield.min.css", rel="stylesheet")
    link(href="static/style.css", rel="stylesheet")
    meta(name="viewport" content="width=device-width, initial-scale=1")
  body
    div.mdl-layout.mdl-js-layout.mdl-layout--fixed-header
      header.mdl-layout__header.transparent-header
        div.mdl-layout__header-row
          span.mdl-layout-title Koffiescript
          div.mdl-layout-spacer
          label.mdl-switch.mdl-js-switch.mdl-js-ripple-effect(for="toggle-cleaner-button")
            input#toggle-cleaner-button.mdl-switch__input(type="checkbox")
            span.mdl-switch__label
      div.mdl-layout__drawer
        span.mdl-layout-title Koffiescript
        nav.mdl-navigation
          a.mdl-navigation__link(href="") Link1
          a.mdl-navigation__link(href="") Link2
          a.mdl-navigation__link(href="") Link3
      main.mdl-layout__content
        div#content
    script(src='https://code.getmdl.io/1.1.3/material.min.js')
    script(src='https://rawgit.com/MEYVN-digital/mdl-selectfield/master/mdl-selectfield.min.js')
    script(src="http://momentjs.com/downloads/moment-with-locales.js")
    script(src='static/bundle.js')
